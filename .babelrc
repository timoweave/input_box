{
  "presets": [
    "env", "react"
  ],
  "plugins": [
    "react-html-attrs",
    "transform-react-jsx",
    "transform-es2015-classes",
    "transform-es2015-template-literals",
    [
      "transform-imports", {
	"react-bootstrap": {
	  "transform": "react-bootstrap/lib/${member}",
	  "preventFullImport": true
	}
      }
    ]
  ]
}
